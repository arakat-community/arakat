        "optional": true
      },
      "impurity": {
        "visible_name": "Karmaşıklık",
        "type_constraint": [
          "string"
        ],
        "set_constraint": [
          "entropy",
          "gini"
        ],
        "default": "gini"
      },
      "lossType": {
        "visible_name": "Kayıp Türü",
        "type_constraint": [
          "string"
