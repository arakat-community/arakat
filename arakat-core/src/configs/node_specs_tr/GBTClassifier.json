        "default": "label"
      },
      "predictionCol": {
        "visible_name": "Tahmin Sütunu",
        "type_constraint": [
          "string"
        ],
        "default": "prediction"
      },
      "maxDepth": {
        "visible_name": "Maksimum Derinlik",
        "type_constraint": [
          "integer"
        ],
        "piecewise_constraint": "maxDepth >= 0",
        "default": 5
      },
      "maxBins": {
        "visible_name": "Maksimum İkili Sayısı",
        "type_constraint": [
          "integer"
        ],
        "piecewise_constraint": "maxBins >= 2",
